
{
  "name": "qrCode",
  "version": "0.2",
  "description": "当前网址转为二维码",
  "browser_action": {
    "default_icon":"images/qrcode.png",
    "default_title": "qrCode",
    "default_popup": "popup.html"
  },
  "background": {
        "scripts": ["js/background.js"]
    },
  "permissions": [
    "activeTab", //访问当前活动选项卡
    "tabs" //访问浏览器选项卡
  ],
  "manifest_version": 2
}


